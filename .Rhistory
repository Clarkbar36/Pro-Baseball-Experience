create_map <- function(t,y){
sub_tbl <- tbl %>% filter(collab_type %in% t & Year %in% y)
leaflet(sub_tbl) %>% addProviderTiles(provider = "CartoDB.Positron") %>%
fitBounds(~min(GRIDLONG), ~min(GRIDLAT), ~max(GRIDLONG), ~max(GRIDLAT)) %>%
addMarkers(lng = ~GRIDLONG, lat = ~GRIDLAT,
popup =  sub_tbl$popup_text,
clusterOptions = markerClusterOptions())
}
# Define UI
ui <- bootstrapPage(
tags$style(type = "text/css", "html, body {width:100%;height:100%}"),
tags$head(tags$style(HTML('#controls {background-color: rgba(0,0,50,0.2);}'))),
leafletOutput("map", width = "100%", height = "100%"),
absolutePanel(id = "controls", class = "panel panel-default", fixed = FALSE,
draggable = TRUE, top = 15, left = "auto", right = 5, bottom = "auto",
width = 350, height = 'auto',
h4("FARS International Collaboration"),
checkboxGroupInput("tps","Collab Type:",choices = types, selected = types, inline = TRUE),
checkboxGroupInput("yrs","Year",choices = years, selected = years, inline = TRUE)
)
)
# Define server logic required to draw a histogram
server <- function(input, output) {
output$map <- renderLeaflet(
create_map(t = input$tps, y = input$yrs )
)
}
# Run the application
shinyApp(ui = ui, server = server)
